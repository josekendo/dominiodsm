<!DOCTYPE html>
<html lang="es">
    <head>
        <meta http-equiv="Content-Type" content="text/html; charset=utf-8"/>
        <meta charset="utf-8">
        <meta name="viewport" content="width=device-width, initial-scale=1">
        <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css">
        <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.2.0/jquery.min.js"></script>
        <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js"></script>
        <title>@ViewBag.Title</title>
        <!--Se quita render(content css y bundles/moderniz)-->
    </head>
    <body>
        <nav class="navbar navbar-default">
            <div class="container-fluid">
                <div class="navbar-header">
                    <a href="@Url.Action("Index", "Administrador")" class="navbar-brand"><b style="color:red;">L</b>ife<b style="color:red;">T</b>ag</a>&nbsp;&nbsp;&nbsp;<b  class="navbar-brand">@ViewBag.Titulop</b>
                </div>
                <ul class="nav navbar-nav navbar-right">
                    <li><a href="@Url.Action("Close", "Administrador")"><span class="glyphicon glyphicon-log-out"></span></a></li>
                    <!--<span class=\"glyphicon glyphicon - earphone\"></span>-->
                </ul>
                <form class="navbar-form navbar-right">
                    <div class="form-group">
                        <select class="form-control" id="sel1">
                            <option>ES</option>
                            <option>EN</option>
                            <option>CH</option>
                            <option>JA</option>
                        </select>
                    </div>
                </form>
</div>
        </nav>
        <div id="body">
            @RenderSection("featured", required: false)
            <section class="content-wrapper main-content clear-fix">
                @RenderBody()
            </section>
        </div>
    </body>
</html>